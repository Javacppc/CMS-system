<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="test-action" extends="struts-default" namespace="/">
    	<!-- 這裡的整合代表Struts自己管理自己的，與spring容器不在一起 -->
    	<action name="test_*" class="com.itcast.test.action.TestAction" method="{1}">
    		<result name="success">/WEB-INF/jsp/test/test.jsp</result>
    	</action>
    </package>
	

</struts>
